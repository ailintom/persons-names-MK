###  https://pnm.uni-mainz.de/ontology#refers_to_place
:refers_to_place rdf:type owl:ObjectProperty ;
                 rdfs:domain :criterion ;
                 rdfs:range <http://www.cidoc-crm.org/cidoc-crm/E53_Place> ;
